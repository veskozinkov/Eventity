<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/round_dialog"
    android:outlineProvider="background">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="@fraction/osd_guideline8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline10" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline11" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@fraction/osd_guideline12" />

    <com.scwang.wave.MultiWaveHeader
        android:id="@+id/waveHeader"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:mwhCloseColor="@android:color/black"
        app:mwhColorAlpha="0.6"
        app:mwhIsRunning="false"
        app:mwhStartColor="@color/colorPrimary"
        app:mwhVelocity="0.9"
        app:mwhWaveHeight="@dimen/wave_height" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/back_FAB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="@color/colorPrimary"
        app:fabCustomSize="@dimen/fab_custom_size"
        app:fabSize="mini"
        app:layout_constraintBottom_toTopOf="@+id/guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="@fraction/fab_buttons_v_bias"
        app:maxImageSize="@dimen/icon_size"
        app:rippleColor="@color/colorRipple"
        app:srcCompat="@drawable/ic_back"
        tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/title_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:gravity="center"
        android:shadowColor="@android:color/black"
        android:shadowDx="@dimen/large_text_shadowDx"
        android:shadowDy="@dimen/large_text_shadowDy"
        android:shadowRadius="@dimen/large_text_shadowRadius"
        android:text="@string/other_settings"
        android:textAlignment="center"
        android:textColor="@color/colorTextWhite"
        android:textSize="@dimen/dialog_title_text_size"
        android:textStyle="bold|italic"
        app:layout_constrainedHeight="true"
        app:layout_constraintBottom_toTopOf="@+id/guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="@fraction/osd_title_v_bias" />

    <TextView
        android:id="@+id/timeFormat_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/time_format"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size1"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline1"
        app:layout_constraintVertical_bias="@fraction/v_bias_100"
        tools:ignore="TextContrastCheck" />

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyle"
        android:layout_width="@dimen/osd_progress_bar_width"
        android:layout_height="@dimen/osd_progress_bar_height"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/timeFormat_Spinner"
        app:layout_constraintEnd_toEndOf="@+id/timeFormat_Spinner"
        app:layout_constraintHorizontal_bias="@fraction/h_bias_100"
        app:layout_constraintStart_toStartOf="@+id/timeFormat_Spinner"
        app:layout_constraintTop_toTopOf="@+id/timeFormat_Spinner" />

    <Spinner
        android:id="@+id/timeFormat_Spinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_spinner_s_margin"
        android:layout_marginEnd="@dimen/osd_spinner_e_margin"
        android:background="@drawable/spinner_round_border_with_arrow"
        android:overlapAnchor="false"
        android:padding="@dimen/osd_spinner_padding"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline9"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/dateFormat_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/date_format"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size1"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:layout_constraintVertical_bias="@fraction/v_bias_100" />

    <Spinner
        android:id="@+id/dateFormat_Spinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_spinner_s_margin"
        android:layout_marginEnd="@dimen/osd_spinner_e_margin"
        android:background="@drawable/spinner_round_border_with_arrow"
        android:overlapAnchor="false"
        android:padding="@dimen/osd_spinner_padding"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline10"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="@dimen/osd_progress_bar_width"
        android:layout_height="@dimen/osd_progress_bar_height"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/dateFormat_Spinner"
        app:layout_constraintEnd_toEndOf="@+id/dateFormat_Spinner"
        app:layout_constraintHorizontal_bias="@fraction/h_bias_100"
        app:layout_constraintStart_toStartOf="@+id/dateFormat_Spinner"
        app:layout_constraintTop_toTopOf="@+id/dateFormat_Spinner" />

    <TextView
        android:id="@+id/notifications_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/notifications1"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size1"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        app:layout_constraintVertical_bias="@fraction/v_bias_100"
        tools:ignore="TextContrastCheck" />

    <Spinner
        android:id="@+id/notifications_Spinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_spinner_s_margin"
        android:layout_marginEnd="@dimen/osd_spinner_e_margin"
        android:background="@drawable/spinner_round_border_with_arrow"
        android:overlapAnchor="false"
        android:padding="@dimen/osd_spinner_padding"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline11"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyle"
        android:layout_width="@dimen/osd_progress_bar_width"
        android:layout_height="@dimen/osd_progress_bar_height"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/notifications_Spinner"
        app:layout_constraintEnd_toEndOf="@+id/notifications_Spinner"
        app:layout_constraintHorizontal_bias="@fraction/h_bias_100"
        app:layout_constraintStart_toStartOf="@+id/notifications_Spinner"
        app:layout_constraintTop_toTopOf="@+id/notifications_Spinner" />

    <TextView
        android:id="@+id/notificationsSchedule_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/notifications_schedule"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size1"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        app:layout_constraintVertical_bias="@fraction/v_bias_100"
        tools:ignore="TextContrastCheck" />

    <Spinner
        android:id="@+id/notificationsSchedule_Spinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/osd_spinner_s_margin"
        android:layout_marginEnd="@dimen/osd_spinner_e_margin"
        android:background="@drawable/spinner_round_border_with_arrow"
        android:overlapAnchor="false"
        android:padding="@dimen/osd_spinner_padding"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline12"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ProgressBar
        android:id="@+id/progressBar4"
        style="?android:attr/progressBarStyle"
        android:layout_width="@dimen/osd_progress_bar_width"
        android:layout_height="@dimen/osd_progress_bar_height"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/notificationsSchedule_Spinner"
        app:layout_constraintEnd_toEndOf="@+id/notificationsSchedule_Spinner"
        app:layout_constraintHorizontal_bias="@fraction/h_bias_100"
        app:layout_constraintStart_toStartOf="@+id/notificationsSchedule_Spinner"
        app:layout_constraintTop_toTopOf="@+id/notificationsSchedule_Spinner" />

    <TextView
        android:id="@+id/infoStringChangeLimit_TextView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/info_character_limit"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size2"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintEnd_toStartOf="@+id/guideline8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <Button
        android:id="@+id/increase_Button1"
        android:layout_width="0dp"
        android:layout_height="@dimen/osd_increase_buttons_height"
        android:layout_marginStart="@dimen/osd_increase_buttons_s_margin"
        android:layout_marginEnd="@dimen/osd_increase_buttons_e_margin"
        android:background="@drawable/round_button"
        android:fontFamily="monospace"
        android:text="@string/increase"
        android:textAlignment="center"
        android:textColor="@color/colorTextWhite"
        android:textSize="@dimen/osd_increase_buttons_text_size"
        android:textStyle="bold|italic"
        app:layout_constrainedHeight="true"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

    <ProgressBar
        android:id="@+id/progressBar5"
        style="?android:attr/progressBarStyle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/increase_Button1"
        app:layout_constraintEnd_toEndOf="@+id/increase_Button1"
        app:layout_constraintStart_toStartOf="@+id/increase_Button1"
        app:layout_constraintTop_toTopOf="@+id/increase_Button1" />

    <TextView
        android:id="@+id/listChangeLimit_TextView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/osd_text_view_s_margin"
        android:fontFamily="monospace"
        android:gravity="start|center_vertical"
        android:text="@string/events_per_day_limit"
        android:textColor="@color/colorTextBlack"
        android:textSize="@dimen/osd_text_size2"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/guideline7"
        app:layout_constraintEnd_toStartOf="@+id/guideline8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline6" />

    <Button
        android:id="@+id/increase_Button2"
        android:layout_width="0dp"
        android:layout_height="@dimen/osd_increase_buttons_height"
        android:layout_marginStart="@dimen/osd_increase_buttons_s_margin"
        android:layout_marginEnd="@dimen/osd_increase_buttons_e_margin"
        android:background="@drawable/round_button"
        android:fontFamily="monospace"
        android:text="@string/increase"
        android:textAlignment="center"
        android:textColor="@color/colorTextWhite"
        android:textSize="@dimen/osd_increase_buttons_text_size"
        android:textStyle="bold|italic"
        app:layout_constrainedHeight="true"
        app:layout_constraintBottom_toTopOf="@+id/guideline7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        tools:ignore="TouchTargetSizeCheck" />

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/increase_Button2"
        app:layout_constraintEnd_toEndOf="@+id/increase_Button2"
        app:layout_constraintStart_toStartOf="@+id/increase_Button2"
        app:layout_constraintTop_toTopOf="@+id/increase_Button2" />

</androidx.constraintlayout.widget.ConstraintLayout>